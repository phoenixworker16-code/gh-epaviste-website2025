generator client {
  provider = "prisma-client-js"
  engineType = "binary"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Demande {
  id             Int      @id @default(autoincrement())
  nom            String
  prenom         String
  telephone      String
  email          String?
  adresse        String
  ville          String
  codePostal     String
  typeVehicule   String
  marque         String?
  modele         String?
  annee          String?
  etatVehicule   String
  description    String?
  dateCreation   DateTime @default(now())
  statut         String   @default("Nouvelle")
  dateIntervention DateTime?
}